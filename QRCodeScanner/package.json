{
  "name": "QRCodeScanner",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "iosx": "npx pod-install && react-native run-ios",
    "pod-reinstall": "cd ios && pod install --repo-update && cd ..",
    "cleanIOSBuild": "cd ios && rm -rf build && xcodebuild clean && cd ..",
    "cleanAndroidBuild": "cd android && ./gradlew clean && cd ..",
    "cleanPods": "cd ios && rm -rf Pods && rm -rf Podfile.lock && cd ..",
    "simulator": "npx pod-install && npx react-native run-ios --simulator=\"iPhone 15 Pro\"",
    "listIOSDevices": "xcrun simctl list devices",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.73.5",
    "react-native-camera-kit": "^13.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
